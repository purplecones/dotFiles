# env
export EDITOR=vim
export PATH=/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin
# source $HOME/.cargo/env

# aliases
## git
alias g='git'
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gp='git push'
alias gt='git log --graph --oneline --all'

## yarn
alias y='yarn'
alias ys='yarn start'
alias yd='yarn dev'

## npm
alias n='npm'

## serverless
alias s='serverless'

## docker-compose
alias dc='docker-compose'

alias python='/usr/local/bin/python3.7'
alias pip='/usr/local/bin/pip3'
